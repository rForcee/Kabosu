package map;
import map.Position;
import map.Spand;
public class Map 
{
	float X,Y,Sx,Sy;
	
	
		Position centre = new Position(X,Y);
		Spand pointSpand = new Spand(Sx,Sy);
	
	void calculeDistance(){
		double tmpCal = (centre.coordY - centre.coordX)+(pointSpand.coordY - pointSpand.coordX);
		Math.sqrt(tmpCal);
	}
}
