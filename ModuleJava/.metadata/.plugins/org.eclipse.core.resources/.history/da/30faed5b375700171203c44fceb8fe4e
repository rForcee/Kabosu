package map;
import map.Position;
import map.Spand;
public class Map 
{
	
	public Map(float X,float Y,float Sx,float Sy){
		
		public Position centre = new Position(X,Y);
		public Spand pointSpand = new Spand(Sx,Sy);
	}
	
	
	public Double calculeRayon(float X,float Y,float Sx,float Sy){
		double tmpCal = (Y - X)+(Sy - Sx);
		 return Math.sqrt(tmpCal);
	}
	
}
