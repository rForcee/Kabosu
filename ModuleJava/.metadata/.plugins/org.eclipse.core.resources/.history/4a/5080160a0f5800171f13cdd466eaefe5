	private int meteo;
	public Image getSkin1() {
		return skin1;
	}

	public void setSkin1(Image skin1) {
		this.skin1 = skin1;
	}

	public int getMeteo() {
		return meteo;
	}

	public void setMeteo(int meteo) {
		this.meteo = meteo;
	}

}
